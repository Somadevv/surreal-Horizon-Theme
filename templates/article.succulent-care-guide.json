          "blocks": {
            "text_rwnQyU": {
              "type": "text",
              "name": "t:names.heading",
              "settings": {
                "text": "<p><strong>Watering</strong></p>",
                "width": "fit-content",
                "max_width": "normal",
                "alignment": "left",
                "type_preset": "h3",
                "font": "var(--font-body--family)",
                "font_size": "1rem",
                "line_height": "normal",
                "letter_spacing": "normal",
                "case": "none",
                "wrap": "pretty",
                "color": "var(--color-foreground)",
