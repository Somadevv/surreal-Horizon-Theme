              "name": "t:names.heading",
              "settings": {
                "text": "<p><strong>Rescuing Aeoniums from Winter Darkness.</strong></p>",
                "width": "fit-content",
                "max_width": "normal",
                "alignment": "left",
                "type_preset": "h3",
                "font": "var(--font-body--family)",
                "font_size": "1rem",
                "line_height": "normal",
