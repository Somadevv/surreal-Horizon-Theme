              "name": "t:names.heading",
              "settings": {
                "text": "<p><strong>Light and Temperature</strong></p>",
                "width": "fit-content",
                "max_width": "normal",
                "alignment": "left",
                "type_preset": "h3",
                "font": "var(--font-body--family)",
                "font_size": "1rem",
                "line_height": "normal",
