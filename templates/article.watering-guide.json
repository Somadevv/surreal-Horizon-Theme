              "type": "text",
              "name": "t:names.heading",
              "settings": {
                "text": "<p><strong>Seasonal Watering Adjustments</strong></p>",
                "width": "fit-content",
                "max_width": "normal",
                "alignment": "left",
                "type_preset": "h3",
                "font": "var(--font-body--family)",
                "font_size": "1rem",
