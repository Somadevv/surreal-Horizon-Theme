            "text_V8zgBP": {
              "type": "text",
              "name": "t:names.heading",
              "settings": {
                "text": "<p><strong>Flowering</strong></p>",
                "width": "fit-content",
                "max_width": "normal",
                "alignment": "left",
                "type_preset": "h3",
                "font": "var(--font-body--family)",
                "font_size": "1rem",
                "line_height": "normal",
